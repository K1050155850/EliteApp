<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#7AC8EA"
    tools:context=".MenuActivity">

    <TextView
        android:id="@+id/textView10"
        android:layout_width="395dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="12dp"
        android:layout_marginRight="12dp"
        android:layout_marginBottom="616dp"
        android:fontFamily="sans-serif-black"
        android:gravity="center"
        android:text="Elige lo que desees pedir"
        android:textSize="34sp"
        android:textColor="@color/white"/>

    <ImageButton
        android:id="@+id/btn_MenuBebida"
        android:layout_width="167dp"
        android:layout_height="127dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="132dp"
        android:layout_marginRight="132dp"
        android:layout_marginBottom="442dp"
        app:srcCompat="@drawable/bebida"
        android:contentDescription="Vamos al menu de bebidas"
        android:onClick="Bebidas"/>

    <ImageButton
        android:id="@+id/btn_MenuCoctel"
        android:layout_width="172dp"
        android:layout_height="135dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="128dp"
        android:layout_marginRight="128dp"
        android:layout_marginBottom="237dp"
        app:srcCompat="@drawable/coctel"
        android:contentDescription="Vamos al menu de cocteles"
        android:onClick="Cocteles"/>

</RelativeLayout>